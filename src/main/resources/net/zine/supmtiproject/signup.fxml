<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="530.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.zine.supmtiproject.controllers.singupController">
   <children>
      <Label layoutX="107.0" layoutY="36.0" text="SIGN UP" textFill="#7898f2">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="fullName" layoutX="51.0" layoutY="92.0" prefHeight="26.0" prefWidth="198.0" promptText="full name" />
      <TextField fx:id="userName" layoutX="51.0" layoutY="133.0" prefHeight="26.0" prefWidth="198.0" promptText="User Name" />
      <TextField fx:id="city" layoutX="51.0" layoutY="177.0" prefHeight="26.0" prefWidth="198.0" promptText="City" />
      <TextField fx:id="age" layoutX="51.0" layoutY="220.0" prefHeight="26.0" prefWidth="198.0" promptText="age" />
      <TextField fx:id="password" layoutX="51.0" layoutY="265.0" prefHeight="26.0" prefWidth="198.0" promptText="password" />
      <TextField fx:id="confirmpassword" layoutX="51.0" layoutY="313.0" prefHeight="26.0" prefWidth="198.0" promptText="confirm password" />
      <Button fx:id="signUpButton" layoutX="48.0" layoutY="400.0" mnemonicParsing="false" onAction="#signUp" prefHeight="34.0" prefWidth="198.0" style="-fx-background-color: #84c7f7; -fx-border-color: #0365f1;" text="sign up" />
      <Button fx:id="backbutton" layoutX="48.0" layoutY="443.0" mnemonicParsing="false" onAction="#back" prefHeight="34.0" prefWidth="198.0" style="-fx-background-color: #fd5068; -fx-border-color: #0365f1;" text="back" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="53.0" layoutY="356.0" prefHeight="18.0" prefWidth="198.0" textFill="RED" />
   </children>
</AnchorPane>
